<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationSequence">
		<success>Always</success>
		<operations>




			<li Class="PatchOperationFindMod">
				<mods>
					<li>Nature's Pretty Sweet</li>
				</mods> 
				<match Class="PatchOperationAdd"> 
					<xpath>/Defs/VCE_Fishing.BiomeTempDef[defName="VCEF_BiomeWarm"]/biomes</xpath>
					<value>
						<li>TKKN_RedwoodForest</li>
						<li>TKKN_Grasslands</li>	

					</value>
				</match>
			</li>

			<li Class="PatchOperationFindMod">
				<mods>
					<li>Nature's Pretty Sweet</li>
				</mods> 
				<match Class="PatchOperationAdd"> 
					<xpath>/Defs/VCE_Fishing.BiomeTempDef[defName="VCEF_BiomeHot"]/biomes</xpath>
					<value>
						<li>TKKN_Desert</li>
						<li>TKKN_Oasis</li>	
						<li>TKKN_VolcanicFlow</li>		
						<li>TKKN_Savanna</li>
					</value>
				</match>
			</li>






		</operations>
	</Operation>


</Patch>

