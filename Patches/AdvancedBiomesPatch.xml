<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationSequence">
		<success>Always</success>
		<operations>
			<li Class="ModCheck.isModLoaded">
				<modName>Advanced Biomes</modName>
				<yourMod>Vanilla Fishing Expanded</yourMod>
			</li>

			<li Class="PatchOperationAdd"> 
				<xpath>/Defs/VCE_Fishing.BiomeTempDef[defName="VCEF_BiomeWarm"]/biomes</xpath>
				<value>
					<li>PoisonForest</li>
					<li>Wasteland</li>
					<li>Wetland</li>
								
				</value>
			</li>
			
			<li Class="PatchOperationAdd"> 
				<xpath>/Defs/VCE_Fishing.BiomeTempDef[defName="VCEF_BiomeHot"]/biomes</xpath>
				<value>
					<li>Savanna</li>
					<li>Volcano</li>				
				</value>
			</li>
			
			

			


		</operations>
	</Operation>


</Patch>

